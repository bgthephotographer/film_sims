<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.chip.Chip 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/genreName"
    style="@style/Widget.Material3.Chip.Filter"
    android:layout_width="wrap_content"
    android:layout_height="38dp"
    android:layout_marginEnd="10dp"
    android:text="Genre"
    android:textColor="@color/simple_chip_text"
    android:textSize="13sp"
    android:fontFamily="sans-serif"
    android:letterSpacing="0.03"
    app:chipBackgroundColor="@color/simple_chip_bg"
    app:chipStrokeWidth="1dp"
    app:chipStrokeColor="#15FFFFFF"
    app:chipCornerRadius="19dp"
    app:chipMinHeight="38dp"
    app:chipStartPadding="18dp"
    app:chipEndPadding="18dp"
    app:textStartPadding="0dp"
    app:textEndPadding="0dp"
    app:ensureMinTouchTargetSize="false"
    app:checkedIconVisible="false" />
