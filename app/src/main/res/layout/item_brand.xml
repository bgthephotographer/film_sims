<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.chip.Chip 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/brandName"
    style="@style/Widget.Material3.Chip.Filter"
    android:layout_width="wrap_content"
    android:layout_height="42dp"
    android:layout_marginEnd="10dp"
    android:text="Brand"
    android:textColor="@color/simple_chip_text"
    android:textSize="14sp"
    android:fontFamily="sans-serif-medium"
    android:letterSpacing="0.02"
    app:chipBackgroundColor="@color/simple_chip_bg"
    app:chipStrokeWidth="1dp"
    app:chipStrokeColor="#18FFFFFF"
    app:chipCornerRadius="21dp"
    app:chipMinHeight="42dp"
    app:chipStartPadding="20dp"
    app:chipEndPadding="20dp"
    app:textStartPadding="0dp"
    app:textEndPadding="0dp"
    app:ensureMinTouchTargetSize="false"
    app:checkedIconVisible="false" />
